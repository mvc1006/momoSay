body {
  background: #FFFFFF; }

.wrap {
  max-width: 1080px;
  /*height: 300px;*/
  margin: 0 auto;
  padding-top: 20px;
  background: #FFFFFF;
  padding-bottom: 50px; }
  .wrap .article {
    width: 95%;
    padding-bottom: 10px;
    height: 180px;
    margin: 0 auto;
    margin-top: 20px;
    padding: 10px;
    border-radius: 10px; }
    .wrap .article .title {
      width: 250px;
      height: 20px;
      font-size: 16px;
      color: #666666;
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap; }
    .wrap .article .cen {
      width: 95%;
      height: 100px;
      background: white;
      padding: 0px 10px;
      margin: 0 auto;
      border-radius: 2px;
      display: flex;
      align-items: center;
      justify-content: center; }
      .wrap .article .cen .des {
        display: -webkit-box;
