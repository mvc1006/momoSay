              .touxiang{
            width: 75px;
            height: 75px;
            border-radius: 50%;
            background: transparent;
            position: relative;
            margin-right: 6px;
            margin-left: 5px;
//          overflow: hidden;
            
            i{
                position: absolute;
                top: 3px;
                right: 3px;
                color: #fabf40;
                font-size: 14px;
            }
            img{
                  width: 100%;
                  height: 100%;
                  border-radius: 50%;
                  display: block;
                  margin: 0 auto;
            }
        }
        
        .nikename{
           width: 140px;
           height: 60px;
        
