<script>
	mui.plusReady(function(){

        mui.ajax({
            url      : 'http://www.internet-web.cn/Main/MainPage',
            type     : 'GET',
            data:{access_token:window.localStorage.getItem('access_token')},
            success  : function(data){
              
				data = JSON.parse(data);
				
			// 音乐故事会~~~~~~~~~~~~~~~
                var musiclist = data.data.musiclist;
              
              for(var i = 0;i<musiclist.length;i++){
              	
              	var SubImg = musiclist[i].SubImg;
              	var Title = musiclist[i].Title;
              	var Desp = musiclist[i].Desp;
				//   订阅量
              	var LookNum = musiclist[i].LookNum;
              	var SourceUrl = musiclist[i].SourceUrl;
				var PubTime = musiclist[i].PubTime;  
              	var ID = musiclist[i].ID;
              	
				var  html = $(
					
				'<div id="tale'+i+'" class="tale mui-row">'+
						'<input id="input" type="text" hidden  value="'+ID+'">'+
			    		'<div class="fenmian mui-col-xs-4 mui-col-sm-4">'+
			    			'<img src="'+SubImg+'"/>'+
			    		'</div>'+
			    		'<div class="des mui-col-xs-7 mui-col-sm-7">'+
			    			'<h5 class="taleTitle">'+Title+'</h5>'+
			    			'<span class="jieshao">简介：'+Desp+'	</span>'+
			    			'<ul>'+
			    				
		    					'<li>'+
		    						'<span>上传时间：</span>'+
		    						'<span class="yuanjia">'+	    							
		    							'<i></i>'+
		    						'</span>'+
		    						'<span></span>'+
		    					'</li>'+
		    					'<li>'+
			    					'<span>订阅量：</span>'+
			    					'<span>'+LookNum+'</span>'+
			    				'</li>'+
			    			'</ul>'+
			    		'</div>'+
		    		'</div>	  ');

			
					 $('#newclass').append(html);
				
				document.getElementById('tale'+i+'').addEventListener('tap',function(){

				var id = $(this).find(input).val();
						
					mui.openWindow({
							url:'subpages/musicplay.html',
							extras: {'idd':id},		         			
						})
								
				});
	              	
              }

			//   ------甄选课堂知识-------------

			var courselist = data.data.courselist;

			for(var j =0;j<courselist.length;j++ ){
				var SubImg = courselist[j].SubImg;
				var Title = courselist[j].Title;
				var Desp = courselist[j].Desp;
				var OldPrice = courselist[j].OldPrice;
				var NewPrice = courselist[j].NewPrice;
				var SubUser = courselist[j].SubUser;
				

				var html1 = $(

					'<div id="each'+j+'" class="each mui-row">'+
						'<div class="img mui-col-xs-4 mui-col-sm-4">'+
							'<img src="'+SubImg+'" alt="" />'+
						'</div>'+
						'<div class="details mui-col-xs-7 mui-col-sm-7">'+
							'<h5>'+Title+'</h5>'+
							'<p>简介：'+Desp+'</p>'+
							'<ul>'+
								'<li>'+
									'<p>价格：<span>￥'+NewPrice+'</span></p>'+
								'</li>'+
								'<li>'+
									'<p>订阅量：<span>'+SubUser+'</span></p>'+
								'</li>'+
							'</ul>		    				'+
						'</div>		    			'+
					'</div>	');


				$('#pick').append(html1);
				
				document.getElementById('each'+j+'').addEventListener('tap',function(){
					  mui.openWindow({
                            url:'./subpages/storyList.html',
                            id:'storyList.html'

                        })
				});



			}
                           
            },
            error    : function(xhr,type,errorThrown){
                mui.toast(type);
            }
        });
        
   
});
</script>
