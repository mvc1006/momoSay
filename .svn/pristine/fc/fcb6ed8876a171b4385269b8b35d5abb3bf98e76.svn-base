.mui-bar{
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.wrap{
//  max-width:1080px;
    /*height: 300px;*/
    margin: 0 auto;
    padding-top: 1px;
    background: #FFFFFF;
    padding-bottom: 50px;
    font-size: 16px;
    color: #666666;
        
    .mui-slider{
        width: 95%;
        height: 130px;
        border-radius: 5px;
        margin: 20px auto;
//      background: #f85415;
        
          
    }

     .jq22-container{
        width: 95%;
        height: 50px;

          background: #fdfafc;

         border-radius: 5px;
//      text-align: left;
//      line-height: 50px;
        padding:0 10px;
        margin: 10px auto;
    }
    
    .kind{
        width: 95%;
        height: auto;
        margin: 20px auto;
//      background: darkcyan;
        border-radius: 10px;
        
        
        
        .class{
            width: 100%;
//          height: 150px;
            padding: 10px;
//          padding-bottom: 50px;
            margin: 0 auto;
            margin-bottom: 20px;
            padding-bottom: 15px;
            padding-top: 15px;
             background: #F3F5F7;
             background: #fdfafc;
            border-radius: 5px;
            
            .title{
                width: 100%;
                height: 50px;
                background: white;
//                background: linear-gradient(to right, #66eef6,#eec33f, #e45f37);
                display: flex;
                align-items: center;
                justify-content: space-between;
                font-size: 16px;
                color: #666666;
                padding: 0 5px;
                border-radius: 5px;
            }
            
            .each{
                width: 100%;
                height: 100px;
                background: white;
                border-radius: 10px;
                margin: 0 auto;
                padding: 10px 0px;
                margin-top: 10px;
                display: flex;
                align-items: center;
                justify-content: space-around;
                
                .img{
                    width: 100px;
                    height: 100px;
//                  background: darkcyan;
                    border-radius: 5px;
                    display: flex;
                    align-items: center;
                    justify-content: center;
                    
                    img{
                        width: 90%;
                        height: 90%;
                         border-radius: 5px;
                    }
                    
                }
                
                .details{
//                  width: 350px;
                    height: 100px;
//                  background: #F3F5F7;
                    display: flex;
//                  align-items: center;
                    flex-direction: column;
                    justify-content: space-around;
                    padding: 10px 10px 5px 5px;
                    font-size: 12px;
                    
                    h5{
                        font-size: 14px;
                        font-weight: 900;
                    }
                    .des{
                        line-height: 20px;
                        width: 95%;
//                      background: darkgray;
                        overflow: hidden;
                        text-overflow:ellipsis;
                        white-space: nowrap;
                    }
                    
                    ul{
                        display: flex;
                        align-items: center;
                        justify-content: space-between;
                        li{
                             display: flex;
                        align-items: center;
                        justify-content: space-between;
                        color: #000000;
                        
                            i{
                                color: red;
                                font-style: italic;
                                font-size: 18px;
                                font-weight: 600;
                            }
                        }
                    }
                    
                }
             
            }
        }
    }
}