.content{
    width: 90%;
    height: 300px;
//  background: darkcyan;
    margin: 100px auto;
    
    
    form{
//      width:90%;
        padding-bottom: 60px;
        padding-top: 50px;
        border-radius: 10px;
        background: transparent;
       
        
        .mui-input-row{
            display: flex;
            align-items: center;
             padding-left: 15px;
        }
        
        

    }
    
    .mui-input-group{
        position: static;
    }
    
    .mui-button-row{
        
        .mui-btn-danger{
            width: 95%;
            height: 50px;
           letter-spacing: 2px;
             font-size: 16px;
          border-radius: 5px;
           padding: 10px 0px;
           margin: 55px auto;
           background: #DDA0DD;
           border:1px solid #DDA0DD;
           color: #FFFFFF;
        }
    }

    
    button{
        display: block;
        text-align: center;
        font-size: 12px;
        color: #F0AD4E;
        margin: 0 auto;
        background: transparent;
        border: none;
    }
}
