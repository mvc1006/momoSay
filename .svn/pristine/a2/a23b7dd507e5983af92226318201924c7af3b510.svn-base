        .tale{
            width: 95%;
            height: 120px;
            border-radius: 5px;
           background: #fdfafc;
            background: #FFFFFF;
            margin: 20px auto;
            display: flex;
            align-items: center;
            justify-content: space-between;
            .fenmian{
                height: 110px;
                background: white;
                border-radius: 5px;
                padding: 2px;
                img{
                    width: 100%;
                    height: 100%;
                }
            }
            
            .des{
                height: 110px;
                display: flex;
                flex-direction: column;
                justify-content: space-around;
                .taleTitle{
                    font-size: 14px;
                    font-weight: 800;
                }
                .jieshao{
                    font-size: 12px;
                    line-height: 20px;
                     overflow: hidden;
                      text-overflow: ellipsis;
                      display: -webkit-box;
                      -webkit-line-clamp: 2;
                      -webkit-box-orient: vertical;
                      color: #8f8f94;
                }
                ul{
                    width: 100%;
//                  background: darkcyan;
                    display: flex;
                    align-items: center;
                    justify-content: flex-start;
                    
                    li{
                        display: flex;
                        align-items: center;
                        justify-content: flex-start;
                        font-size: 12px;
                        margin-left: 20px;
                        color: #8f8f94;
                    }
                    
                    li:first-child{
                      
                        margin-left: 0px;
                        .yuanjia{
//                          width: 30px;
//                          background: darkcyan;
                            text-decoration :line-through;
                        }
                        
                        span:last-child{
                            font-size: 16px;
                            font-weight: 800;
                            color: red;
                            font-style:italic;
                            display: block;
                            margin-left: 10px;
                           
                        }
                    }
                    
                    li:last-child{
                        display: flex;
                        align-items: center;
                        justify-content: space-around;
                        
                        
                    }
                }
            }
        }
        
        .tale:nth-child(2){
            margin-top: 0px;
        }
        
        
    }
    
    
    
    
